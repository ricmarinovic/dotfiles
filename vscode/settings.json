{
  "breadcrumbs.enabled": false,
  "debug.enableAllHovers": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [98, 220],
  "editor.scrollbar.vertical": "visible",
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 98,
  "emmet.includeLanguages": {"erb": "html", "html-eex": "html"},
  "explorer.autoReveal": true,
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": true,
  "explorer.enableDragAndDrop": true,
  "explorer.openEditors.visible": 9,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "html.format.endWithNewline": true,
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.exclude": {
    "**/.elixir_ls": true,
    "_build/*": true,
    "deps": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Fira Code",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "window.newWindowDimensions": "fullscreen",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  "window.titleBarStyle": "custom",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.panel.defaultLocation": "right",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.centerLayout": false,
  "[markdown]": {
    "editor.tabSize": 4,
    "editor.wordWrap": "bounded",
    "editor.quickSuggestions": true,
    "editor.suggest.showWords": false,
    "editor.wordWrapColumn": 130,
    "editor.rulers": [130]
  },
  "[elixir]": {
    "editor.quickSuggestions": true,
    "editor.formatOnSave": true
  },
  "[git-commit]": {
    "editor.rulers": [80],
    "editor.wordWrapColumn": 80
  },
  "[todo]": {
    "editor.tabSize": 4,
    "editor.wordWrapColumn": 130,
    "editor.rulers": [130]
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.wordWrap": "off",
    "editor.rulers": [125],
  },
  "[erb]": {
    "editor.wordWrap": "off"
  },
  "ruby.lint": {
    "rubocop": {
      "lint": true,
      "rails": true
    }
  },
  "ruby.lintDebounceTime": 500,
  "[python]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true
  },
  "[c]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true
  },
  "[asm]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.keymap": "none",
  "gitlens.views.repositories.enabled": false,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.keyboardNavigation": "filter",
  "telemetry.enableTelemetry": false,
  "workbench.colorTheme": "Academie Syntax",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "task.autoDetect": "off",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 15,
  "terminal.integrated.allowMnemonics": true,
  "scm.alwaysShowProviders": true,
  "git.autoRepositoryDetection": "openEditors",
  "debug.console.closeOnEnd": true,
  "debug.console.fontSize": 15,
  "debug.openDebug": "openOnDebugBreak",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "docked",
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.openExplorerOnEnd": true,
  "debug.onTaskErrors": "showErrors",
  "debug.internalConsoleOptions": "neverOpen",
  "files.associations": {
    "*.html.leex": "html-eex"
  },
  "todo.file.name": "project.todo",
  "todo.embedded.provider": "rg",
  "todo.file.view.expanded": false,
  "todo.colors.comment": "#81a1c1",
  "todo.colors.project": "#83AFE5",
  "todo.colors.projectStatistics": "#81a1c1",
  "todo.colors.code": "#9a93e1",
  "todo.colors.cancelled": "#f58c80",
  "todo.colors.done": "#a8ce93",
  "todo.colors.tag": "#ebcb8b",
  "todo.colors.tags.background": ["#f58c80", "#d8a13b", "#ebcb8b", "#9a93e1"],
  "todo.file.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/public/**",
    "**/tmp/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**"
  ],
  "todo.embedded.exclude": [
    "**/.*",
    "**/.*/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/tmp/**",
    "**/public/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/CHANGELOG",
    "**/CHANGELOG.*",
    "**/*.min.*",
    "**/*.map",
    "**/*.{3ds,3g2,3gp,7z,a,aac,adp,ai,aif,aiff,alz,ape,apk,ar,arj,asf,au,avi,bak,baml,bh,bin,bk,bmp,btif,bz2,bzip2,cab,caf,cgm,class,cmx,cpio,cr2,csv,cur,dat,dcm,deb,dex,djvu,dll,dmg,dng,doc,docm,docx,dot,dotm,dra,DS_Store,dsk,dts,dtshd,dvb,dwg,dxf,ecelp4800,ecelp7470,ecelp9600,egg,eol,eot,epub,exe,f4v,fbs,fh,fla,flac,fli,flv,fpx,fst,fvt,g3,gif,graffle,gz,gzip,h261,h263,h264,icns,ico,ief,img,ipa,iso,jar,jpeg,jpg,jpgv,jpm,jxr,key,ktx,lha,lib,lvp,lz,lzh,lzma,lzo,m3u,m4a,m4v,mar,mdi,mht,mid,midi,mj2,mka,mkv,mmr,mng,mobi,mov,movie,mp3,mp4,mp4a,mpeg,mpg,mpga,mxu,nef,npx,numbers,o,oga,ogg,ogv,otf,pages,pbm,pcx,pdb,pdf,pea,pgm,pic,png,pnm,pot,potm,potx,ppa,ppam,ppm,pps,ppsm,ppsx,ppt,pptm,pptx,psd,pya,pyc,pyo,pyv,qt,rar,ras,raw,resources,rgb,rip,rlc,rmf,rmvb,rtf,rz,s3m,s7z,scpt,sgi,shar,sil,sketch,slk,smv,so,sub,swf,tar,tbz,tbz2,tga,tgz,thmx,tif,tiff,tlz,ttc,ttf,txz,udf,uvh,uvi,uvm,uvp,uvs,uvu,viv,vob,war,wav,wax,wbmp,wdp,weba,webm,webp,whl,wim,wm,wma,wmv,wmx,woff,woff2,wvx,xbm,xif,xla,xlam,xls,xlsb,xlsm,xlsx,xlt,xltm,xltx,xm,xmind,xpi,xpm,xwd,xz,z,zip,zipx}"
  ],
  "liveshare.presence": true,
  "editor.occurrencesHighlight": false,
  "workbench.activityBar.visible": true
}
