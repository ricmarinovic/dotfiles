{
  "debug.enableAllHovers": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    98
  ],
  "editor.scrollbar.vertical": "visible",
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 98,
  "emmet.includeLanguages": {
    "erb": "html",
    "html-eex": "html"
  },
  "explorer.autoReveal": true,
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": true,
  "explorer.enableDragAndDrop": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "html.format.endWithNewline": true,
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.exclude": {
    "**/.elixir_ls": true,
    "_build/*": true,
    "deps": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Fira Code",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "window.newWindowDimensions": "fullscreen",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "window.titleBarStyle": "custom",
  "workbench.editor.showTabs": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.panel.defaultLocation": "right",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.centerLayout": false,
  "[markdown]": {
    "editor.tabSize": 4,
    "editor.wordWrap": "bounded",
    "editor.quickSuggestions": true,
    "editor.suggest.showWords": false,
    "editor.wordWrapColumn": 130,
    "editor.rulers": [
      130
    ]
  },
  "[elixir]": {
    "editor.quickSuggestions": true,
    "editor.formatOnSave": true
  },
  "[git-commit]": {
    "editor.rulers": [
      80
    ],
    "editor.wordWrapColumn": 80
  },
  "[todo]": {
    "editor.tabSize": 4,
    "editor.wordWrapColumn": 130,
    "editor.rulers": [
      130
    ]
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.wordWrap": "off",
    "editor.rulers": [
      125
    ]
  },
  "[erb]": {
    "editor.wordWrap": "off"
  },
  "ruby.lint": {
    "rubocop": {
      "lint": true,
      "rails": true
    }
  },
  "ruby.lintDebounceTime": 500,
  "[python]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true
  },
  "[c]": {
    "editor.tabSize": 4
  },
  "[asm]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.keymap": "none",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.keyboardNavigation": "filter",
  "telemetry.enableTelemetry": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html-eex]": {
    "editor.wordWrap": "off"
  },
  "task.autoDetect": "off",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "terminal.integrated.allowMnemonics": true,
  "scm.alwaysShowProviders": true,
  "git.autoRepositoryDetection": "openEditors",
  "debug.console.closeOnEnd": true,
  "debug.openDebug": "openOnDebugBreak",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "docked",
  "debug.openExplorerOnEnd": true,
  "debug.onTaskErrors": "showErrors",
  "debug.internalConsoleOptions": "neverOpen",
  "files.associations": {
    "*.html.leex": "html-eex"
  },
  "liveshare.presence": true,
  "gitlens.advanced.blame.delayAfterEdit": 350,
  "markdown.preview.fontSize": 15,
  "workbench.colorTheme": "Dark Elixir",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "editor.occurrencesHighlight": false,
  "scm.alwaysShowRepositories": true,
  "editor.accessibilitySupport": "off",
  "liveshare.comments": false,
  "workbench.editor.enablePreview": false,
  "window.zoomLevel": 1,
  "editor.fontSize": 15,
  "scm.inputFontSize": 15,
  "debug.console.fontSize": 15,
  "terminal.integrated.fontSize": 15,
  "breadcrumbs.enabled": false
}
