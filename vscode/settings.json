{
  "breadcrumbs.enabled": false,
  "debug.enableAllHovers": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [98, 220],
  "editor.scrollbar.vertical": "visible",
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 98,
  "emmet.includeLanguages": {"erb": "html", "HTML (EEx)": "html"},
  "explorer.autoReveal": true,
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": true,
  "explorer.enableDragAndDrop": true,
  "explorer.openEditors.visible": 9,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "html.format.endWithNewline": true,
  "search.exclude": {
    "**/.elixir_ls": true,
    "_build/*": true,
    "deps": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Fira Code",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "window.newWindowDimensions": "fullscreen",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  "window.titleBarStyle": "custom",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.panel.defaultLocation": "right",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.centerLayout": false,
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.quickSuggestions": false,
    "editor.wordWrapColumn": 100,
    "editor.rulers": [100]
  },
  "[elixir]": {
    "editor.quickSuggestions": true,
    "editor.formatOnSave": true
  },
  "[git-commit]": {
    "editor.rulers": [80],
    "editor.wordWrapColumn": 80
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.wordWrap": "off"
  },
  "[erb]": {
    "editor.wordWrap": "off"
  },
  "ruby.lint": {
    "rubocop": {
      "lint": true,
      "rails": true
    }
  },
  "ruby.lintDebounceTime": 500,
  "[python]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.tabSize": 4
  },
  "[c]": {
    "editor.tabSize": 4
  },
  "[asm]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.keymap": "none",
  "gitlens.views.repositories.enabled": false,
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.experimental.templateInterpolationService": false,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.keyboardNavigation": "filter",
  "telemetry.enableTelemetry": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "scm.defaultViewMode": "tree",
  "workbench.colorTheme": "Academie Syntax",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "task.autoDetect": "off",
  "files.watcherExclude": {
    "**/node_modules": true
  },
  "editor.fontSize": 15
}
