{
  "def": {
    "prefix": "def",
    "body": [
      "def ${1:name}($2) do",
      "\t$0",
      "end"
    ]
  },

  "defp": {
    "prefix": "defp",
    "body": [
      "defp ${1:name}($2) do",
      "\t$0",
      "end"
    ]
  },

  "idef": {
    "prefix": "idef",
    "body": [
      "def ${1:name}($2), do: $0"
    ]
  },

  "idefp": {
    "prefix": "idefp",
    "body": [
      "def ${1:name}($2), do: $0"
    ]
  },

  "Annonymous function": {
    "prefix": "af",
    "description": "Annonymous function",
    "body": [
      "fn (${1:}) -> $2 end$3"
    ]
  },

  "pry": {
    "prefix": "pry",
    "body": [
      "require IEx; IEx.pry"
    ]
  },

  "todo": {
    "prefix": "todo",
    "body": [
      "// TODO: $0"
    ]
  },

  "puts": {
    "prefix": "puts",
    "body": [
      "IO.puts ${1:\"$2\"}"
    ]
  },

  "test": {
    "prefix": "test",
    "body": [
      "test \"$1\" do",
      "\t$0",
      "end"
    ]
  },

  "describe": {
    "description": "describe test",
    "prefix": "describe",
    "body": [
      "describe $1 do",
      "\t$2",
      "end"
    ]
  },

  "case": {
    "prefix": "case",
    "body": [
      "case $1 do",
      "\t$0",
      "end"
    ]
  },

  "do": {
    "prefix": "do",
    "body": [
      "do",
      "\t$0",
      "end"
    ]
  },

  "defmodule": {
    "prefix": "defmodule",
    "body": [
      "defmodule $1 do",
      "\t$0",
      "end"
    ]
  },

  "Module constant": {
    "prefix": "mod",
    "description": "Module constant",
    "body": "__MODULE__"
  },
}
