{
  "idef": {
    "prefix": "idef",
    "body": [
      "def ${1:name}($2), do: $0"
    ]
  },
  "idefp": {
    "prefix": "idefp",
    "body": [
      "def ${1:name}($2), do: $0"
    ]
  },
  "Annonymous function": {
    "prefix": "af",
    "description": "Annonymous function",
    "body": [
      "fn (${1:}) -> $2 end$3"
    ]
  },
  "pry": {
    "prefix": "pry",
    "body": [
      "require IEx",
      "IEx.pry",
      "$0"
    ]
  },
  "puts": {
    "prefix": "puts",
    "body": [
      "IO.puts ${1:\"$2\"}"
    ]
  },
  "inspect": {
    "prefix": "ii",
    "body": [
      "IO.inspect($1, label: \"$1\")"
    ]
  },
  "test": {
    "prefix": "test",
    "body": [
      "test \"$1\" do",
      "\t$0",
      "end"
    ]
  },
  "describe": {
    "description": "describe test",
    "prefix": "describe",
    "body": [
      "describe $1 do",
      "\t$2",
      "end"
    ]
  },
  "Module constant": {
    "prefix": "mod",
    "description": "Module constant",
    "body": "__MODULE__"
  }
