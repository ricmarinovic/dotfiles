{
  "Arrow function": {
    "prefix": "af",
    "description": "Fat arrow function",
    "body": [
      "($1) => {",
      "\t$2",
      "}"
    ]
  },

  "getElementById": {
    "prefix": "geti",
    "description": "Get element by id",
    "body": "document.getElementById(\"$1\")"
  },

  "Console log": {
    "prefix": "log",
    "description": "Console log",
    "body": "console.log($1);"
  },

  "Class": {
    "prefix": "class",
    "description": "Class",
    "body": [
      "class $1 {",
      "\t$2",
      "}"
    ]
  },

  "Import": {
    "prefix": "imp",
    "description": "Import",
    "body": "import $1 from '$2'"
  },

  "Import destructuring": {
    "prefix": "impd",
    "description": "Import with destructuring",
    "body": "import { $1 } from '$2'"
  },

  "Object method": {
    "prefix": "kf",
    "body": [
      "${1:functionName}($2) {",
      "\t$3",
      "}"
    ]
  },

  "Object key": {
    "prefix": "kv",
    "body": "${1:key}: $2"
  },

  "React Component": {
    "prefix": "component",
    "body": [
      "class $1 extends React.Component {",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$2",
      "\t\t)",
      "\t}",
      "}"
    ]
  },

  "React PureComponent": {
    "prefix": "purecomponent",
    "body": [
      "class $1 extends React.PureComponent {",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$2",
      "\t\t)",
      "\t}",
      "}"
    ]
  },

  "React Constructor": {
    "prefix": "rconstructor",
    "body": [
      "constructor(props) {",
      "\tsuper(props)\n",
      "\t$1",
      "}\n"
    ]
  },

  "React boiler": {
    "prefix": "react",
    "body": [
      "import React from 'react'\n",
      "export default class $1 extends React.Component {",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$2",
      "\t\t)",
      "\t}",
      "}"
    ]
  },

  "Import react": {
    "prefix": "impreact",
    "body": "import React from 'react'"
  }
}
